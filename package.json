{
  "name": "@buttercup/datasources",
  "version": "4.0.0",
  "description": "Buttercup archive datasources",
  "main": "source/index.js",
  "scripts": {
    "docs": "jsdoc2md 'source/**/*.js' > API.md",
    "format": "prettier --write '{source,test}/**/*.js'",
    "precommit": "lint-staged",
    "test": "run-s test:unit test:format",
    "test:format": "prettier-check '{source,test}/**/*.js'",
    "test:unit": "mocha -r test/index.js 'test/**/*.spec.js'"
  },
  "files": [
    "source/**/*.js",
    "*.md"
  ],
  "lint-staged": {
    "{source,test}/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buttercup/datasources.git"
  },
  "keywords": [
    "Buttercup",
    "datasource",
    "owncloud",
    "dropbox",
    "nextcloud",
    "webdav",
    "archive"
  ],
  "author": "Perry Mitchell <perry@perrymitchell.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buttercup/datasources/issues"
  },
  "homepage": "https://github.com/buttercup/datasources#readme",
  "devDependencies": {
    "@buttercup/app-env": "^0.1.1",
    "buttercup": "^2.15.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "file-exists": "^5.0.1",
    "husky": "^3.0.8",
    "jsdoc-to-markdown": "^5.0.1",
    "lint-staged": "^9.4.1",
    "mocha": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "prettier-check": "^2.0.0",
    "rewire": "^4.0.1",
    "rimraf": "^2.7.1",
    "sinon": "^7.5.0"
  },
  "peerDependencies": {
    "@buttercup/app-env": ">= 0.1.1"
  },
  "dependencies": {
    "@buttercup/dropbox-client": "^0.4.0",
    "@buttercup/googledrive-client": "^0.8.0",
    "@buttercup/signing": "^0.1.0",
    "foreachasync": "^5.1.3",
    "global": "^4.4.0",
    "hash.js": "^1.1.7",
    "pify": "^4.0.1",
    "url-join": "^4.0.1",
    "verror": "^1.10.0",
    "webdav": "^2.9.1"
  }
}
