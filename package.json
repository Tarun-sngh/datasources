{
  "name": "@buttercup/datasources",
  "version": "1.0.1",
  "description": "Buttercup archive datasources",
  "main": "source/index.js",
  "scripts": {
    "docs": "jsdoc2md 'source/**/*.js' > API.md",
    "format": "prettier --write '{source,test}/**/*.js'",
    "precommit": "lint-staged",
    "test": "run-s test:unit test:format",
    "test:format": "prettier-check '{source,test}/**/*.js'",
    "test:unit": "mocha -r test/index.js 'test/**/*.spec.js'"
  },
  "lint-staged": {
    "{source,test}/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buttercup/datasources.git"
  },
  "keywords": [
    "Buttercup",
    "datasource",
    "owncloud",
    "dropbox",
    "nextcloud",
    "webdav",
    "archive"
  ],
  "author": "Perry Mitchell <perry@perrymitchell.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buttercup/datasources/issues"
  },
  "homepage": "https://github.com/buttercup/datasources#readme",
  "devDependencies": {
    "buttercup": "^1.6.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "file-exists": "^5.0.1",
    "husky": "^0.14.3",
    "iocane": "^0.10.1",
    "jsdoc-to-markdown": "^4.0.1",
    "lint-staged": "^7.0.0",
    "mocha": "^5.0.4",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "prettier-check": "^2.0.0",
    "rewire": "^3.0.2",
    "rimraf": "^2.6.2",
    "sinon": "^4.4.6"
  },
  "peerDependencies": {
    "buttercup": "1.*",
    "iocane": "0.*"
  },
  "dependencies": {
    "@buttercup/signing": "^0.1.0",
    "dropbox-fs": "^0.2.0",
    "gzip-js": "^0.3.2",
    "pify": "^3.0.0",
    "url-join": "^4.0.0",
    "webdav": "^1.4.0"
  }
}
